<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Dashboard theme -->
    <style name="DashboardTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/dashboardColorPrimary</item>
        <item name="colorPrimaryDark">@color/dashboardColorPrimaryDark</item>
        <item name="colorAccent">@color/dashboardColorAccent</item>
    </style>
</resources>
